<%- include ('partials/header')%>
    <%- include ('partials/navbar')%>
        <h1>
            <%= title %>
        </h1>
        <div class="container">
            <div class="row">
                <div class="col-md-4">
                    <div class="card text-bg-light">
                        <img src="/images/animoji.png" class="card-img-top" alt="My avatar">
                        <div class="card-body">
                            <h5 class="card-title">Aazain Ullah Khan</h5>
                        </div>
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item text-bg-light">
                                <p class="fw-semibold">SCHOOL</p>
                                <p>Centennial College</p>
                            </li>
                            <li class="list-group-item text-bg-light">
                                <p class="fw-semibold">EMAIL</p>
                                <p>aazainkhan@gmail.com</p>
                            </li>
                        </ul>
                        <div class="card-body">
                            <a href="https://www.github.com/aazainkhan/" target="_blank" class="social-link"><ion-icon
                                    name="logo-github"></ion-icon></a>
                            <a href="https://www.linkedin.com/in/aazainkhan/" target="_blank"
                                class="social-link"><ion-icon name="logo-linkedin"></ion-icon></a>
                        </div>
                    </div>
                </div>
                <div class="col-md-8">
                    <div class="card text-bg-light">
                        <div class="card-body">
                            <h3 class="card-title">Get in touch!</h3>
                            <form>
                                <div class="mb-3">
                                    <input type="text" class="form-control" id="firstName" name="firstName"
                                        placeholder="First Name">
                                </div>
                                <div class="mb-3">
                                    <input type="text" class="form-control" id="lastName" name="lastName"
                                        placeholder="Last Name">
                                </div>
                                <div class="mb-3">
                                    <input type="email" class="form-control" id="email" name="email"
                                        placeholder="Email">
                                </div>
                                <div class="mb-3">
                                    <input type="tel" class="form-control" id="phone" name="phone"
                                        placeholder="Phone Number">
                                </div>
                                <div class="mb-3">
                                    <textarea class="form-control" id="message" name="message" rows="5"
                                        placeholder="Message"></textarea>
                                </div>
                                <button type="submit" class="btn btn-primary">Submit</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%- include ('partials/footer')%>